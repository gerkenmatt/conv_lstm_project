{
	"data": {
		"filename": "square_saw.csv",
		"columns": [
			"Close"
		],
		"sequence_length": 80,
		"train_test_split": 0.5,
		"normalise": true
	},
	"training": {
		"epochs": 20,
		"batch_size": 32
	},
	"model": {
		"learning":0.01,
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 100,
				"input_timesteps": 49,
				"input_dim": 1,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	},
	"func_model": {
		"learning":0.007,
		"filters": 64,
		"kernel" : 5, 
		"dropout": 0.3
	}
}